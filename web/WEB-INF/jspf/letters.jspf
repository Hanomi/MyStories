<div>
    <jsp:useBean id="letterList" scope="application" class="ru.invictus.mystory.web.beans.LetterList"/>
    <%
        for (char letter : letterList.getLetters()) {
    %>
    <a class="<%=(request.getParameter("letter") != null && request.getParameter("letter").charAt(0) == letter) ? "pushed2" : "pushed"%>"
       href="<%=request.getContextPath() + "/main.jsp?letter=" + letter%>">
        <%=letter%>
    </a>
    <%}%>
</div>